{
    "name": "blog-post-creator",
    "displayName": "Blog Post Creator",
    "description": "Create new blog posts with formatted filenames",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:blog.createPost"
    ],
    "main": ".vscode/blog-post.js",
    "contributes": {
        "commands": [
            {
                "command": "blog.createPost",
                "title": "Create New Blog Post"
            }
        ]
    }
}